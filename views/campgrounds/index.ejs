<!--Just give the folder name where the boilerplate is -->

<% layout('layouts/boilerplate') %>
    <h1>All campground list</h1>
    <div><a href="/campgrounds/new">Create new campground</a></div>
    <ul>
        <% for (let camps of campgrounds) { %>
            <li>
                <a href="/campgrounds/<%=camps._id%>">
                    <%=camps.title%>
                </a>
            </li>
        <% } %>
    </ul>
